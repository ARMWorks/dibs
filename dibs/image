function image_config()
{
	CONFIG_IMAGE_PARTITION_BOOT_TYPE=
	if [[ "y" = "$CONFIG_IMAGE_PARTITION_BOOT_TYPE_EXT2" ]] ; then
		CONFIG_IMAGE_PARTITION_BOOT_TYPE="ext2"
	elif [[ "y" = "$CONFIG_IMAGE_PARTITION_BOOT_TYPE_EXT3" ]] ; then
		CONFIG_IMAGE_PARTITION_BOOT_TYPE="ext3"
	elif [[ "y" = "$CONFIG_IMAGE_PARTITION_BOOT_TYPE_EXT4" ]] ; then
		CONFIG_IMAGE_PARTITION_BOOT_TYPE="ext4"
	elif [[ "y" = "$CONFIG_IMAGE_PARTITION_BOOT_TYPE_FAT32" ]] ; then
		CONFIG_IMAGE_PARTITION_BOOT_TYPE="fat32"
	fi

	CONFIG_IMAGE_PARTITION_ROOT_TYPE=
	if [[ "y" = "$CONFIG_IMAGE_PARTITION_ROOT_TYPE_EXT2" ]] ; then
		CONFIG_IMAGE_PARTITION_ROOT_TYPE="ext2"
	elif [[ "y" = "$CONFIG_IMAGE_PARTITION_ROOT_TYPE_EXT3" ]] ; then
		CONFIG_IMAGE_PARTITION_ROOT_TYPE="ext3"
	elif [[ "y" = "$CONFIG_IMAGE_PARTITION_ROOT_TYPE_EXT4" ]] ; then
		CONFIG_IMAGE_PARTITION_ROOT_TYPE="ext4"
	fi
}

function image_generate()
{
	true
}
